pipeline {
    agent { node {label 'linux-rohit'}} 
    parameters {
        string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Hello Dnyneshwar Sir this is pipeline created my rohit patil?')
    }
    stages {
        stage('Build') {
            steps { 
            script {
            sh "rm -rf game-of-life"
            sh "git clone https://github.com/Dee601/game-of-life game-of-life"
                echo 'Build Successfully'
                   }
            }
        }                    
    }
}
